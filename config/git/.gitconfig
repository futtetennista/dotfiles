[alias]
        h = log -p --follow
	s = status
	co = checkout
	cm = commit -m
	po = push origin
	pr = pull -r -p
	amend = commit --amend
	size = count-objects -vH
	zip = archive --format=zip --output project.zip HEAD
	undo = reset HEAD~
        root = rev-parse --show-toplevel
[push]
        autoSetupRemote = true
[rebase]
        autoStash = true
